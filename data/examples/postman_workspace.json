{
  "collections": [
    {
      "info": {
        "name": "API Collection",
        "description": "A sample API collection",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
        "_postman_id": "api-collection-id"
      },
      "item": [
        {
          "name": "Get Users",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{base_url}}/users",
              "protocol": "https",
              "host": ["{{base_url}}"],
              "path": ["users"]
            }
          },
          "response": [],
          "_postman_id": "get-users-id"
        },
        {
          "name": "Get User by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{base_url}}/users/{{user_id}}",
              "protocol": "https",
              "host": ["{{base_url}}"],
              "path": ["users", "{{user_id}}"]
            }
          },
          "response": [],
          "_postman_id": "get-user-by-id-id"
        }
      ]
    }
  ],
  "environments": [
    {
      "id": "dev-environment-id",
      "name": "Development Environment",
      "values": [
        {
          "key": "base_url",
          "value": "https://jsonplaceholder.typicode.com",
          "type": "default",
          "enabled": true
        },
        {
          "key": "user_id",
          "value": "1",
          "type": "default",
          "enabled": true
        }
      ],
      "_postman_variable_scope": "environment"
    },
    {
      "id": "prod-environment-id",
      "name": "Production Environment",
      "values": [
        {
          "key": "base_url",
          "value": "https://api.production.com",
          "type": "default",
          "enabled": true
        },
        {
          "key": "user_id",
          "value": "1",
          "type": "default",
          "enabled": true
        }
      ],
      "_postman_variable_scope": "environment"
    }
  ]
} 